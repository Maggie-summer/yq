table-column prop=\"title\" label=\"标题\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"sectionname\" label=\"所属分类\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"作者\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-button\r\n            @click=\"handleDetail(row)\"\r\n            plain\r\n            icon=\"el-icon-search\"\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n          >详情</el-button>\r\n          <el-button icon=\"el-icon-edit\" size=\"mini\" plain type=\"warning\">编辑</el-button>\r\n          <el-button\r\n            @click=\"handleDelete(row)\"\r\n            plain\r\n            size=\"mini\"\r\n            icon=\"el-icon-delete\"\r\n            type=\"danger\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"display: flex;justify-content:center;padding: 20px 0;\">\r\n      <el-pagination\r\n        :page-size=\"pageSize\"\r\n        :current-page.sync=\"pageIndex\"\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :total=\"count\"\r\n      ></el-pagination>\r\n    </div>\r\n    <!-- 富文本 -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport { getArticlelist,delarticle } from \"@/api/index.js\";\r\nexport default {\r\n\r\n  methods: {\r\n    async getArticlelist() {\r\n      let res = await getArticlelist(this.pageIndex, this.pageSize);\r\n      // console.log(res.data);\r\n      this.tableData = res.data;\r\n    },\r\n\r\n    handleDetail(rowData) {\r\n      console.log(rowData.id);\r\n    },\r\n    // 删除\r\n    async handleDelete(rowData) {\r\n      await this.$confirm(\r\n        `此操作将永久删除${rowData.title}`,\r\n        \"是否继续?\",\r\n        \"提示\",\r\n        {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\"\r\n        }\r\n      );\r\n        let res = await delarticle(rowData.id);\r\n        if (res.data.success) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功!\"\r\n          });\r\n          this.getArticlelist();\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: \"删除失败!\"\r\n          });\r\n        }\r\n    }\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      count: 0,\r\n      pageIndex: 1,\r\n      pageSize: 10\r\n    };\r\n  },\r\n  created() {\r\n    this.getArticlelist();\r\n  },\r\n  watch: {\r\n    pageIndex() {\r\n      this.getArticlelist(this.pageIndex, this.pageSize);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.addarticle .tinymce{\r\n  display: none;\r\n}\r\n</style>"]}]}