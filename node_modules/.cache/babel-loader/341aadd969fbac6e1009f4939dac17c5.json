"编号\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"名称\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"creator\" label=\"文案\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-button\r\n            @click=\"handleDetail(row)\"\r\n            plain\r\n            icon=\"el-icon-search\"\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n          >详情</el-button>\r\n          <el-button icon=\"el-icon-edit\" size=\"mini\" plain type=\"warning\">编辑</el-button>\r\n          <el-button\r\n            @click=\"handleDelete(row)\"\r\n            plain\r\n            size=\"mini\"\r\n            icon=\"el-icon-delete\"\r\n            type=\"danger\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 弹框 -->\r\n    <el-dialog title=\"添加菜单\" @close=\"handleCloseModal\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n      <el-form :model=\"navmenu\" :rules=\"rules\" ref=\"dialogForm\">\r\n        <el-form-item label=\"名称\" prop=\"name\" label-width=\"70px\">\r\n          <el-input v-model=\"navmenu.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"文案\" prop=\"creator\" label-width=\"70px\">\r\n          <el-input v-model=\"navmenu.creator\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">保 存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getMenu ,delMenu,savemenu} from \"@/api/index.js\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      dialogVisible: false,\r\n      navmenu: { name: \"\", creator: \"\" },\r\n         rules: {\r\n        name: [\r\n          { required: true, message: \"请输入姓名\", trigger: \"blur\" },\r\n          { min: 3, max: 12, message: \"长度在 3 到 12 个字符\", trigger: \"blur\" }\r\n        ],\r\n        creator: [\r\n          { required: true, message: \"请输文案\", trigger: \"blur\" },\r\n          { min: 7, max: 20, message: \"长度在 7 到 20 个字符\", trigger: \"blur\" }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n\r\n  methods: {\r\n      //清楚弹框内容\r\n    handleCloseModal() {\r\n      this.navmenu = {\r\n        name: \"\", creator: \"\"\r\n      };\r\n      // 对整个表单进行重置\r\n      // this.$refs.dialogForm.resetFields();\r\n    },\r\n      save() {\r\n      //   console.log(this.$refs);\r\n      this.$refs.dialogForm.validate(async valid => {\r\n        if (valid) {\r\n          let res = await savemenu(this.navmenu);\r\n          if (res.data.success)\r\n            this.$message({\r\n              message: \"保存成功！\",\r\n              type: \"success\"\r\n            });\r\n          else {\r\n            this.$message({\r\n              message: \"保存失败！\",\r\n              type: \"success\"\r\n            });\r\n          }\r\n\r\n          this.dialogVisible = false;\r\n        } else {\r\n          this.$message.error(\"请输入完整信息！\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n// 获取数据\r\n    async getMenu() {\r\n      let res = await getMenu();\r\n      // console.log(res.data);\r\n      this.tableData = res.data;\r\n    },\r\n\r\n    handleDetail(rowData) {\r\n      console.log(rowData.id);\r\n    },\r\n    // 删除\r\n    async handleDelete(rowData) {\r\n      await this.$confirm(\r\n        `此操作将永久删除此条信息`,\r\n        \"是否继续?\",\r\n        \"提示\",\r\n        {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\"\r\n        }\r\n      );\r\n        let res = await delMenu(rowData.id);\r\n        if (res.data.success) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功!\"\r\n          });\r\n          this.getMenu();\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: \"删除失败!\"\r\n          });\r\n        }\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.getMenu();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>"],"sourceRoot":"src/view/user"}]}