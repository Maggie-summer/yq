AA;AACA,KA7CA;AA+CA,IAAA,IA/CA,kBA+CA;AAAA;;AACA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,QAAA,CAAA,iBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KADA;AAAA;AAAA;AAAA;;AAAA;AAAA,gDAEA,YAAA,CAAA,KAAA,CAAA,QAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;AAGA,oBAAA,GAAA,CAAA,IAAA,CAAA,OAAA,EACA,KAAA,CAAA,QAAA,CAAA;AACA,kBAAA,OAAA,EAAA,OADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA,EADA,KAKA;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA;AACA,oBAAA,OAAA,EAAA,OADA;AAEA,oBAAA,IAAA,EAAA;AAFA,mBAAA;AAIA;AAEA,gBAAA,KAAA,CAAA,aAAA,GAAA,KAAA;AAfA;AAAA;;AAAA;AAiBA,gBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA;;AAjBA,kDAkBA,KAlBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;AAqBA,KAtEA;AAuEA;AACA,IAAA,YAxEA,wBAwEA,OAxEA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8CAyEA,KAAA,QAAA,6EAAA,OAAA,EAAA,IAAA,EAAA;AACA,gBAAA,iBAAA,EAAA,IADA;AAEA,gBAAA,gBAAA,EAAA,IAFA;AAGA,gBAAA,IAAA,EAAA;AAHA,eAAA,CAzEA;;AAAA;AAAA;AAAA,8CA8EA,WAAA,CAAA,OAAA,CAAA,EAAA,CA9EA;;AAAA;AA8EA,cAAA,GA9EA;;AA+EA,kBAAA,GAAA,CAAA,IAAA,CAAA,OAAA,EAAA;AACA,qBAAA,QAAA,CAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;AAIA,qBAAA,eAAA;AACA,eANA,MAMA;AACA,qBAAA,QAAA,CAAA;AACA,kBAAA,IAAA,EAAA,OADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;AAIA;;AA1FA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAvCA;AAqIA,EAAA,KAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,WAAA,eAAA,CAAA,KAAA,SAAA,EAAA,KAAA,QAAA;AACA;AAHA;AArIA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-button\r\n      icon=\"el-icon-circle-plus-outline\"\r\n      type=\"primary\"\r\n      style=\"margin:10px 0;\"\r\n      @click=\"dialogVisible=true\"\r\n    >添加讲师</el-button>\r\n\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\">\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"180\"></el-table-column>\r\n      <el-table-column  prop=\"img\" label=\"头像\" width=\"180\">\r\n        <!-- <template slot-scope=\"scope\">\r\n          <img v-for=\"(item,index) in scope.row.number\" :key=\"index\" class=\"scopeItem\" src=\"\" alt=\"\">  \r\n        </template> -->\r\n\r\n      </el-table-column>\r\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"姓名\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-button\r\n            @click=\"handleDetail(row)\"\r\n            plain\r\n            icon=\"el-icon-search\"\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n          >详情</el-button>\r\n          <el-button icon=\"el-icon-edit\" size=\"mini\" plain type=\"warning\">编辑</el-button>\r\n          <el-button\r\n            @click=\"handleDelete(row)\"\r\n            plain\r\n            size=\"mini\"\r\n            icon=\"el-icon-delete\"\r\n            type=\"danger\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"display: flex;justify-content:center;padding: 20px 0;\">\r\n      <el-pagination\r\n        :page-size=\"pageSize\"\r\n        :current-page.sync=\"pageIndex\"\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :total=\"count\"\r\n      ></el-pagination>\r\n    </div>\r\n    <!-- 弹框 -->\r\n    <el-dialog title=\"添加讲师\" @close=\"handleCloseModal\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n      <el-form :model=\"jiangshi\" :rules=\"rules\" ref=\"dialogForm\">\r\n        <el-form-item label=\"头像\" prop=\"img\" label-width=\"70px\">\r\n          <el-upload\r\n            ref=\"upload\"  \r\n            class=\"avatar-uploader\"\r\n            action=\"http://47.92.50.43:8888/user/uploadimg\"\r\n            :show-file-list=\"false\"\r\n            :on-success=\"handleAvatarSuccess\"\r\n            :before-upload=\"beforeAvatarUpload\"\r\n          >\r\n            <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"昵称\" prop=\"nickname\" label-width=\"70px\">\r\n          <el-input v-model=\"jiangshi.nickname\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"姓名\" prop=\"name\" label-width=\"70px\">\r\n          <el-input v-model=\"jiangshi.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"方向\" prop=\"fangxiang\" label-width=\"70px\">\r\n          <el-input v-model=\"jiangshi.fangxiang\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"描述\" prop=\"desc\" label-width=\"70px\">\r\n          <el-input v-model=\"jiangshi.desc\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"save\">保 存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getJiangshiList,\r\n  getJiangshiCount,\r\n  delJiangshi,\r\n  savejiangshi\r\n} from \"@/api/index.js\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      count: 0,\r\n      pageIndex: 1,\r\n      pageSize: 10,\r\n      dialogVisible: false,\r\n      imageUrl: \"\",\r\n      jiangshi: {\r\n        img: \"\",\r\n        nickname: \"\",\r\n        fangxiang: \"\",\r\n        name: \"\",\r\n        desc: \"\"\r\n      },\r\n      rules: {\r\n        nickname: [\r\n          { required: true, message: \"请输入昵称\", trigger: \"blur\" },\r\n          { min: 3, max: 12, message: \"长度在 3 到 12 个字符\", trigger: \"blur\" }\r\n        ],\r\n        name: [\r\n          { required: true, message: \"请输入姓名\", trigger: \"blur\" },\r\n          { min: 7, max: 20, message: \"长度在 3 到 12 个字符\", trigger: \"blur\" }\r\n        ],\r\n        nafangxiang: [\r\n          { required: true, message: \"请输入方向\", trigger: \"blur\" },\r\n          { min: 7, max: 20, message: \"长度在 7 到 20 个字符\", trigger: \"blur\" }\r\n        ],\r\n        desc: [\r\n          { required: true, message: \"请输入描述\", trigger: \"blur\" },\r\n          { min: 7, max: 20, message: \"长度在 7 到 20 个字符\", trigger: \"blur\" }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getJiangshiList();\r\n  },\r\n  methods: {\r\n    async getJiangshiList() {\r\n      this.getJiangshiCount();\r\n      let res = await getJiangshiList(this.pageIndex, this.pageSize);\r\n      // console.log(res.data);\r\n      this.tableData = res.data;\r\n    },\r\n    async getJiangshiCount() {\r\n      let res = await getJiangshiCount();\r\n      // console.log(res)\r\n      this.count = res.data.count;\r\n    },\r\n    handleDetail(rowData) {\r\n      console.log(rowData.id);\r\n    },\r\n    // 添加讲师\r\n    //清楚弹框内容\r\n    handleCloseModal() {\r\n      this.jiangshi = {\r\n        img: \"\",\r\n        nickname: \"\",\r\n        fangxiang: \"\",\r\n        name: \"\",\r\n        desc: \"\"\r\n      };\r\n      // 对整个表单进行重置\r\n      this.$refs.dialogForm.resetFields();\r\n      this.$refs.upload.clearFiles();\r\n\r\n    },\r\n    handleAvatarSuccess(res, file) {\r\n      this.jiangshi.img = res\r\n      this.imageUrl = URL.createObjectURL(file.raw);\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === \"image/jpeg\";\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n      console.log(111);\r\n      if (!isJPG) {\r\n        this.$message.error(\"上传头像图片只能是 JPG 格式!\");\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 2MB!\");\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n\r\n    save() {\r\n      //   console.log(this.$refs);\r\n      this.$refs.dialogForm.validate(async valid => {\r\n        if (valid) {\r\n          let res = await savejiangshi(this.jiangshi);\r\n          if (res.data.success)\r\n            this.$message({\r\n              message: \"保存成功！\",\r\n              type: \"success\"\r\n            });\r\n          else {\r\n            this.$message({\r\n              message: \"保存失败！\",\r\n              type: \"success\"\r\n            });\r\n          }\r\n\r\n          this.dialogVisible = false;\r\n        } else {\r\n          this.$message.error(\"请输入完整信息！\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    // 删除\r\n    async handleDelete(rowData) {\r\n      await this.$confirm(`此操作将永久删除此条信息`, \"是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      });\r\n      let res = await delJiangshi(rowData.id);\r\n      if (res.data.success) {\r\n        this.$message({\r\n          type: \"success\",\r\n          message: \"删除成功!\"\r\n        });\r\n        this.getJiangshiList();\r\n      } else {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"删除失败!\"\r\n        });\r\n      }\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    pageIndex() {\r\n      this.getJiangshiList(this.pageIndex, this.pageSize);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.avatar-uploader .el-upload {\r\n  z-index: 999;\r\n  border: 1px solid #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>"],"sourceRoot":"src/view/user"}]}