,GAAA;;AAlDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoDA;AACA,IAAA,YArDA,wBAqDA,OArDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8CAsDA,KAAA,QAAA,2DACA,OAAA,CAAA,IADA,GAEA,OAFA,EAGA,IAHA,EAIA;AACA,gBAAA,iBAAA,EAAA,IADA;AAEA,gBAAA,gBAAA,EAAA,IAFA;AAGA,gBAAA,IAAA,EAAA;AAHA,eAJA,CAtDA;;AAAA;AAAA;AAAA,8CAgEA,SAAA,CAAA,OAAA,CAAA,EAAA,CAhEA;;AAAA;AAgEA,cAAA,GAhEA;;AAiEA,kBAAA,GAAA,CAAA,IAAA,CAAA,OAAA,EAAA;AACA,qBAAA,QAAA,CAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;AAIA,qBAAA,SAAA;AACA,eANA,MAMA;AACA,qBAAA,QAAA,CAAA;AACA,kBAAA,IAAA,EAAA,OADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;AAIA;;AA5EA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAvBA;AAuGA,EAAA,OAvGA,qBAuGA;AACA,SAAA,SAAA;AACA;AAzGA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-button\r\n      icon=\"el-icon-circle-plus-outline\"\r\n      type=\"primary\"\r\n      style=\"margin:10px 0;\"\r\n      @click=\"addfriend\"\r\n    >添加友情链接</el-button>\r\n    <el-table :data=\"tableData\" border style=\"width: 100%;\">\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"网站名称\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"path\" label=\"网站地址\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"{row}\">\r\n          <el-button\r\n            @click=\"handleEdilt(row)\"\r\n            icon=\"el-icon-edit\"\r\n            size=\"mini\"\r\n            plain\r\n            type=\"warning\"\r\n          >编辑</el-button>\r\n          <el-button\r\n            @click=\"handleDelete(row)\"\r\n            plain\r\n            size=\"mini\"\r\n            icon=\"el-icon-delete\"\r\n            type=\"danger\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 弹框 -->\r\n    <el-dialog :title=\"mode\" @close=\"handleCloseModal\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n      <el-form :model=\"friend\" :rules=\"rules\" ref=\"dialogForm\">\r\n        <el-form-item label=\"名称\" prop=\"name\" label-width=\"80px\">\r\n          <el-input v-model=\"friend.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\" prop=\"path\" label-width=\"80px\">\r\n          <el-input v-model=\"friend.path\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\" v-show=\"mode==='添加友情链接' || mode==='编辑友情链接'\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">保 存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getFriend,\r\n  delFriend,\r\n  Ediltdetailfl,\r\n  saveFriend\r\n} from \"@/api/index.js\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      mode: \"\",\r\n      tableData: [],\r\n      dialogVisible: false,\r\n      navmenu: { name: \"\", path: \"\" },\r\n      friend: {\r\n        name: \"\",\r\n        path: \"\"\r\n      },\r\n      rules: {\r\n        name: [\r\n          { required: true, message: \"请输入姓名\", trigger: \"blur\" },\r\n          { min: 3, max: 12, message: \"长度在 3 到 12 个字符\", trigger: \"blur\" }\r\n        ],\r\n        path: [\r\n          { required: true, message: \"请输入地址\", trigger: \"blur\" },\r\n          { min: 3, max: 12, message: \"长度在 3 到 20 个字符\", trigger: \"blur\" }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    addfriend() {\r\n      this.dialogVisible = true;\r\n      this.mode = \"添加友情链接\";\r\n    },\r\n    //清楚弹框内容\r\n    handleCloseModal() {\r\n      this.friend = {\r\n        name: \"\",\r\n        path: \"\"\r\n      };\r\n      // 对整个表单进行重置\r\n      this.$refs.dialogForm.resetFields();\r\n    },\r\n    save() {\r\n      //   console.log(this.$refs);\r\n      this.$refs.dialogForm.validate(async valid => {\r\n        if (valid) {\r\n          let res = await saveFriend(this.friend);\r\n          if (res.data.success) {\r\n            this.$message({\r\n              message: \"保存成功！\",\r\n              type: \"success\"\r\n            });\r\n            this.getFriend();\r\n          } else {\r\n            this.$message({\r\n              message: \"保存失败！\",\r\n              type: \"success\"\r\n            });\r\n          }\r\n\r\n          this.dialogVisible = false;\r\n        } else {\r\n          this.$message.error(\"请输入完整信息！\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    async getFriend() {\r\n      let res = await getFriend();\r\n      console.log(res.data);\r\n      this.tableData = res.data;\r\n    },\r\n\r\n    async handleEdilt(rowData) {\r\n      this.dialogVisible = true;\r\n      this.mode = \"编辑友情链接\";\r\n      let res = await Ediltdetailfl(rowData.id);\r\n      this.friend = res.data;\r\n      console.log(res);\r\n    },\r\n    // 删除\r\n    async handleDelete(rowData) {\r\n      await this.$confirm(\r\n        `此操作将永久删除${rowData.name}`,\r\n        \"是否继续?\",\r\n        \"提示\",\r\n        {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\"\r\n        }\r\n      );\r\n      let res = await delFriend(rowData.id);\r\n      if (res.data.success) {\r\n        this.$message({\r\n          type: \"success\",\r\n          message: \"删除成功!\"\r\n        });\r\n        this.getFriend();\r\n      } else {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"删除失败!\"\r\n        });\r\n      }\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.getFriend();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>"],"sourceRoot":"src/view/user"}]}